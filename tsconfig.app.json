{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "types/env.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "public/static/extends/jUtilsBase/umd/index.d.ts",
    "public/static/extends/jUtilsBrowser/umd/index.d.ts"
    
  ],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "types": [
      "element-plus/global"
    ],
    "paths": {
      "@/*": ["./src/*"],
      "@bestime/nine-ui-vue3": ["./src/plugins/jcy/nine-ui-vue3"],
      "@bestime/scroll-bar": ["./src/plugins/jcy/scroll-bar"]
    }
  }
}
